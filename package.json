{
  "name": "packtris",
  "version": "0.0.1",
  "private": true,
  "description": "Neptun Software - Frontend",
  "main": "server.js",
  "scripts": {
    "serve": "node .",
    "build:test": "rm -rf dist/* && NODE_ENV=test webpack --progress",
    "build:stage": "rm -rf dist/* && NODE_ENV=stage webpack --progress",
    "build:production": "rm -rf dist/* && NODE_ENV=production webpack --progress",
    "lint:js": "eslint src"
  },
  "author": "Noel Schnierer <noel.schnierer@execart.de>",
  "dependencies": {
    "classnames": "^2.2.5",
    "flexboxgrid": "^6.3.0",
    "intl": "^1.2.4",
    "lodash": "^4.14.1",
    "moment": "^2.17.1",
    "normalizr": "^2.2.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-flexbox-grid": "^0.10.2",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.6",
    "react-router": "^2.6.1",
    "react-toolbox": "^2.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-saga": "^0.13.0",
    "simple-bp3d-node": "^0.1.0",
    "uuid": "^2.0.3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-fs-extra": "^0.8.2",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.2",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "rules": {
      "strict": [
        2,
        "never"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "plugins": [
      "react"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
